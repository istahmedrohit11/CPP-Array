#include<iostream>
using namespace std;

int main() {
    int arr[50], size;
    
    // Input size of array
    cout << "Enter the size of: ";
    cin >> size;
    
    // Input elements of the array
    cout << "Enter the elements of array: ";
    for(int i = 0; i < size; i++) {
        cin >> arr[i];
    }
    
    // Output elements of the array
    cout << "The elements are: ";
    for(int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }

    return 0;
}
